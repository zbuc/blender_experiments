{
  "reconstruction": {
    "reconstruction_mode": "loft_profile",
    "unit_scale": 0.01,
    "num_slices": 200
  },
  "mesh_join": {
    "mode": "boolean",
    "boolean_solver": "auto"
  },
  "silhouette_extract_ref": {
    "prefer_alpha": true,
    "alpha_threshold": 127,
    "gray_threshold": null,
    "invert_policy": "auto",
    "morph_close_px": 0,
    "morph_open_px": 0,
    "fill_holes": false,
    "largest_component_only": false
  },
  "silhouette_extract_render": {
    "prefer_alpha": true,
    "alpha_threshold": 127,
    "gray_threshold": null,
    "invert_policy": "auto",
    "morph_close_px": 0,
    "morph_open_px": 0,
    "fill_holes": false,
    "largest_component_only": false
  },
  "profile_sampling": {
    "num_samples": 2048,
    "sample_policy": "endpoints",
    "fill_strategy": "interp_linear",
    "smoothing_window": 3
  },
  "mesh_from_profile": {
    "radial_segments": 192,
    "cap_mode": "fan",
    "min_radius_u": 0.0005,
    "merge_threshold_u": 0.0005,
    "recalc_normals": true,
    "shade_smooth": true,
    "weld_degenerate_rings": true,
    "apply_decimation": true,
    "decimate_ratio": 0.1,
    "decimate_method": "COLLAPSE",
    "use_top_contour": true,
    "contour_simplify_epsilon": 0.001,
    "fallback_to_elliptical": true
  },
  "render_silhouette": {
    "resolution": [
      4096,
      4096
    ],
    "engine": "BLENDER_EEVEE",
    "transparent_bg": true,
    "samples": 64,
    "margin_frac": 0.1,
    "color_mode": "RGBA",
    "force_material": false,
    "background_color": [
      1.0,
      1.0,
      1.0,
      1.0
    ],
    "silhouette_color": [
      0.0,
      0.0,
      0.0,
      1.0
    ],
    "camera_distance_factor": 2.0,
    "party_mode": true
  },
  "canonicalize": {
    "output_size": 256,
    "padding_frac": 0.1,
    "anchor": "bottom_center",
    "interp": "nearest"
  },
  "silhouette_intersection": {
    "extrude_distance": 1.0,
    "contour_mode": "external",
    "largest_component_only": null,
    "silhouette_extract_override": null,
    "boolean_solver": "auto"
  }
}
